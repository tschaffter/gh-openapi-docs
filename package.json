{
  "name": "@ga4gh/gh-openapi-docs",
  "description": "Build ReDoc API documentation for an OpenAPI specification and host on GitHub Pages",
  "author": "James Eddy <james.a.eddy@gmail.com>",
  "version": "0.3.0",
  "license": "Apache-2.0",
  "repository": "github:ga4gh/gh-openapi-docs",
  "main": "./src/lib/index.js",
  "scripts": {
    "fetch": "node src/fetchpages.js",
    "build:swagger": "node src/swagger-ui.js",
    "build:redoc": "node src/redoc-ui.js",
    "clean": "rm -rf dist/* bin/*",
    "build": "npx webpack-cli --mode=development",
    "cleanBuild": "npm run clean && npm run build",
    "test": "npx mocha-webpack --webpack-config webpack.config-test.js --recursive \"src\" \"test/mocha\" --exit",
    "coverage": "npx nyc npm run test",
    "pushCoverageReport": "npx nyc report --reporter=text-lcov | npx coveralls",
    "lint:all": "echo 1"
  },
  "bin": "./dist/bundle.js",
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@redocly/openapi-cli": "0.12.16",
    "chalk": "4.1.1",
    "copy-webpack-plugin": "^8.1.1",
    "git-repo-info": "^2.1.1",
    "is-ci": "^3.0.0",
    "json-refs": "^3.0.15",
    "loader-utils": "^2.0.0",
    "lodash": "^4.17.21",
    "oas-linter": "^3.2.1",
    "oas-resolver": "^2.5.4",
    "oas-validator": "^5.0.5",
    "redoc-cli": "^0.11.4",
    "remote-origin-url": "^2.0.0",
    "shelljs": "^0.8.4",
    "swagger-ui-dist": "3.48.0",
    "update-notifier": "5.1.0",
    "yargs-parser": "^20.2.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/compat-data": "^7.14.0",
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.13.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.14.1",
    "@babel/register": "^7.13.16",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "istanbul-instrumenter-loader": "^3.0.1",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "pkg": "^5.1.0",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-node-externals": "^3.0.0"
  },
  "nyc": {
    "instrument": false,
    "sourceMap": false
  },
  "publishConfig": {
    "access": "public"
  }
}
